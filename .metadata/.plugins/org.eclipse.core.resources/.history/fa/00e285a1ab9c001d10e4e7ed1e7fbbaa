package com.aurionpro.model;

public class Rectangle {
	private double height;
	private double width;

	public void setHeight(double ht) {
		if (height < 1) {
			this.height = 1;
		} else if (height > 100) {
			this.height = 100;
		} else {
			this.height = height;
		}
		this.height = ht;
	}

	public double getHeight() {
		return height;

	}

	public void setWidth(double wt) {
		this.width = wt;
	}

	public double getWidth() {
		return width;
	}
}